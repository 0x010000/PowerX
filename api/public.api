@server(
    group: public
    prefix: /api/public/v1
)

service PowerX {
    @doc "活码"
    @handler AccessLiveQRCode
    get /live-qr-code/:uid (AccessLiveQRCodeRequest) returns (AccessLiveQRCodeReply)
}

type (
    AccessLiveQRCodeRequest {
        Uid string `path:"uid"`
    }

    AccessLiveQRCodeReply {
        RedirectTo string `json:"redirectTo"`
    }
)